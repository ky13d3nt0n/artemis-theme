{
  "$schema": "vscode://schemas/color-theme",
  "author": "Kyle Denton",
  "name": "Artemis",
  "colorSpaceName": "sRGB",
  "semanticClass": "theme.dark.artemis_syntax",
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#5C6370",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "scope": [
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    }
  ],
  "colors": {
    "activityBar.background": "#000000",
    "sideBar.background": "#000000",
    "sideBar.foreground": "#D4D7D6",
    "sideBar.border": "#8DC149",
    "sideBarSectionHeader.background": "#1F2326",
    "tab.activeBackground": "#000000",
    "tab.activeBorder": "#8DC149",
    "editor.background": "#1F2326",
    "editorLineNumber.foreground": "#4D5A5E",
    "editorLineNumber.activeForeground": "#4D5A5E",
    "statusBar.background": "#0E1112",
    "statusBar.foreground": "#8DC149",
    "breadcrumb.foreground": "#FFFFFF",
    "breadcrumb.background": "#000000"
  }
}